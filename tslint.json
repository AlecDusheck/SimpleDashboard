{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "no-implicit-dependencies": [true, "dev"],
    "no-submodule-imports": [true,"@angular/core", "@angular/router", "@angular/common", "zone.js/dist/zone",
      "zone.js/dist/zone-testing", "@angular/platform-browser-dynamic/testing", "core-js/es7/reflect"],
    "object-literal-sort-keys": false, // imo this is annoying as you have to alphabetize JSON
    "interface-name": false // TODO: maybe fix... having an I{name} for interfaces is kinda nice but typings file is weird...
  }
}
